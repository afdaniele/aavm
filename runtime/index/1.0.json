    {
        "schema": "1.0",
        "version": "0.0.1",
        "description": "AAVM Runtime based on Ubuntu 20.04",
        "maintainer": "Andrea F. Daniele <afdaniele@ttic.edu>",
        "image": {
            "repository": "aavm",
            "tag": "ubuntu-20.04",
            "user": "afdaniele",
            "arch": [
                "amd64",
                "arm64v8"
            ]
        },
        "configuration": {
            "tmpfs": {
                "/tmp": "",
                "/run": "",
                "/run/lock": ""
            },
            "volumes": {
                "/sys/fs/cgroup": {
                    "bind": "/sys/fs/cgroup",
                    "mode": "ro"
                }
            },
            "privileged": true
        }
    },
